{
  "name": "yourdash",
  "author": "Ewsgit",
  "license": "MIT",
  "private": true,
  "workspaces": [
    "instance/web",
    "instance/backend",
    "website",
    "uikit",
    "tunnel",
    "apps/*",
    "instanceManager"
  ],
  "dependencies": {
    "prettier": "^3.5.3"
  },
  "scripts": {
    "backend": "yarn --cwd ./instance/backend dev",
    "web": "yarn --cwd ./instance/web dev",
    "website": "yarn --cwd ./website dev",
    "build-deps": "yarn --cwd ./tunnel build && yarn --cwd ./uikit build"
  },
  "packageManager": "yarn@4.9.1"
}
